{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "textdomain": "chart-builder-controller",
  "name": "prc-block/chart-builder-controller",
  "category": "media",
  "title": "Chart Builder Controller",
  "description": "Create a custom data-driven chart using blocks.",
  "icon": "chart-area",
  "keywords": [
    "chart"
  ],
  "attributes": {
    "id": {
      "type": "string"
    },
    "isStatic": {
      "type": "boolean",
      "default": false
    },
    "isFreeform": {
      "type": "boolean",
      "default": false
    },
    "isTable": {
      "type": "boolean",
      "default": false
    },
    "isInteractive": {
      "type": "boolean",
      "default": true
    },
    "chartType": {
      "type": "string"
    },
    "transformed": {
      "type": "boolean",
      "default": false
    },
    "isConvertedChart": {
      "type": "boolean",
      "default": false
    },
    "tabsActive": {
      "type": "boolean",
      "default": true
    },
    "shareActive": {
      "type": "boolean",
      "default": true
    },
    "chartPreformattedData": {
      "type": "array"
    },
    "align": {
      "type": "string",
      "default": "none"
    }
  },
  "example": {
    "attributes": {
      "chartType": "bar",
      "className": "is-style-bar is-example"
    }
  },
  "providesContext": {
    "prc-chart-builder/id": "id",
    "prc-chart-builder/align": "align"
  },
  "usesContext": [
    "refId"
  ],
  "supports": {
    "html": false,
    "interactivity": true,
    "inserter": false
  },
  "viewScriptModule": "file:./view.js",
  "editorScript": "file:./index.js",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}